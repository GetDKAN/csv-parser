{
    "name": "getdkan/csv-parser",
    "description": "A state-machine driven, CSV parser and validator.",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "fmizzell",
            "email": "fmizzell.dev@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "fmizzell/maquina": "dev-php-81",
        "getdkan/contracts": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.3 <8.5 || >8.5.14 <9.5",
        "rector/rector": "^0.15.17",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/paul-m/maquina"
        }
    ],
    "autoload": {
        "psr-4": {
            "CsvParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CsvParserTest\\": "test/"
        }
    },
    "scripts": {
        "phpcbf": "./vendor/bin/phpcbf",
        "phpcs": "./vendor/bin/phpcs",
        "rector": "./vendor/bin/rector process",
        "rector-dry-run": "./vendor/bin/rector process --dry-run",
        "test": "./vendor/bin/phpunit --testsuite all"
    }
}
